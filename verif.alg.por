programa
{
	
	funcao inicio()
	{
		cadeia algoritmo

		escreva ("Escreva a palavra: ")
		leia (algoritmo)

			se (algoritmo == "Algoritmo" ou algoritmo == "algoritmo") {
				escreva ("ALG")
			}
			senao{
				escreva ("A palavcra não é algoritmo")
			}
	}
}
